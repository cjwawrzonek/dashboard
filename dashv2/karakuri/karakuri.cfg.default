# karakuri
log             karakuri.log
log-level       DEBUG
mongo-uri       mongodb://localhost:27017
pid             /tmp/karakuri.pid
rest-port       8080

# jira
jira-username   jirausername
jira-password   jirapassword

# sfdc
sfdc-username   sfdcusername
sfdc-password   sfdcpassword (remember to prefix with security hash!)

# aws
aws-key         awskey
aws-secret      awsecret

# throttle limits
global-limit    100
user-limit      10
company-limit   3
